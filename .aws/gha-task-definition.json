{
    "containerDefinitions": [
        {
            "name": "flask-appka",
            "image": "saaverdo/flask:stable",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "appk",
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "FLASK_CONFIG",
                    "value": "dev"
                }            
            ],
            "secrets": [
                {
                    "name": "MYSQL_HOST",
                    "valueFrom": "arn:aws:ssm:eu-central-1:452630976589:parameter/demo/prod/db/MYSQL_HOST"
                }
            ],            
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/gha-new",
                    "awslogs-create-group": "true",
                    "awslogs-region": "eu-central-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "family": "gha-new",
    "executionRoleArn": "arn:aws:iam::452630976589:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}